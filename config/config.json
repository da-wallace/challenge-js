{
  "development": {
    "dialect": "sqlite",
    "storage": "./database.sqlite3"
  },
  "production": {
    "dialect": "postgres",
    "protocol": "postgres",
    "dialectOptions": {
        "ssl": {
          "require": true,
          "rejectUnauthorized": false
        }
    },
    "use_env_variable": "DATABASE_URL"
  }
}
